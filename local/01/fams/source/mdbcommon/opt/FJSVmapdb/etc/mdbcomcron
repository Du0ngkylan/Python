SHELL=/bin/bash
PATH=/sbin:/bin:/usr/sbin:/usr/bin
MAILTO=""
0 1 * * * root /usr/bin/find /var/log/mapdb -type f -mtime +2 -print0 | xargs -0 -r -n 1 -P 0 -I {} \rm "{}" > /dev/null 2>&1
